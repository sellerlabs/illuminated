{
  "name": "sellerlabs/illuminated",
  "description": "Library of Laravel 5 utilities.",
  "license": "MIT",
  "require": {
    "illuminate/auth": "^5.0",
    "illuminate/support": "^5.0",
    "illuminate/session": "^5.0",
    "illuminate/contracts": "^5.0",
    "illuminate/view": "^5.0",
    "illuminate/database": "^5.0",
    "tijsverkoyen/css-to-inline-styles": "~1.5",
    "sellerlabs/nucleus": "~0.5.0",
    "league/commonmark": "^0.12",
    "vlucas/phpdotenv": "~1.0"
  },
  "authors": [
    {
      "name": "Eduardo Trujillo",
      "email": "ed@roundsphere.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SellerLabs\\Illuminated\\": "src/SellerLabs/Illuminated"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SellerLabs\\Illuminated\\": "tests/SellerLabs/Illuminated",
      "Tests\\SellerLabs\\Support\\": "tests/SellerLabs/Support"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "laravel/framework": "^5.0",
    "phpunit/phpunit": "~4.6@dev",
    "mockery/mockery": "0.9.*@dev",
    "doctrine/dbal": "~2.3",
    "sellerlabs/php-standard": "~0.1.10"
  }
}
